#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/esteban/workspace/ts-pg-interceptor/node_modules/.pnpm/vite-node@0.30.1_@types+node@18.15.10/node_modules/vite-node/node_modules:/Users/esteban/workspace/ts-pg-interceptor/node_modules/.pnpm/vite-node@0.30.1_@types+node@18.15.10/node_modules:/Users/esteban/workspace/ts-pg-interceptor/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/esteban/workspace/ts-pg-interceptor/node_modules/.pnpm/vite-node@0.30.1_@types+node@18.15.10/node_modules/vite-node/node_modules:/Users/esteban/workspace/ts-pg-interceptor/node_modules/.pnpm/vite-node@0.30.1_@types+node@18.15.10/node_modules:/Users/esteban/workspace/ts-pg-interceptor/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../vite-node.mjs" "$@"
else
  exec node  "$basedir/../../vite-node.mjs" "$@"
fi
